net: "models/face_48_cal/train_val.prototxt"
test_iter: 40
test_interval:500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.01
momentum: 0.9
weight_decay: 0.008
# The learning rate policy
lr_policy: "step"
stepsize: 40000
gamma: 0.5
display: 50
max_iter: 400000
snapshot: 10000
snapshot_prefix: "models/face_48_cal/face_48_cal_train"
solver_mode: GPU
