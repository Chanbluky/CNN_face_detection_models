net: "models/face_24_cal/train_val.prototxt"
test_iter: 40
test_interval:2500
# The base learning rate, momentum and the weight decay of the network.
base_lr: 0.005
momentum: 0.9
weight_decay: 0.008
# The learning rate policy
lr_policy: "step"
stepsize: 30000
gamma: 0.5
display: 500
max_iter: 400000
snapshot: 100000
snapshot_prefix: "models/face_24_cal/face_24_cal_train"
solver_mode: GPU
